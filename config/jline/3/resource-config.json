{
  "resources": {
    "includes": [
      {
        "pattern": ".*org/jline/utils/capabilities.txt$",
        "condition": {"typeReachable": "org.jline.terminal.TerminalBuilder"}
      },
      {
        "pattern": ".*org/jline/utils/colors.txt$",
        "condition": {"typeReachable": "org.jline.terminal.TerminalBuilder"}
      },
      {
        "pattern": ".*org/jline/utils/.*.caps$",
        "condition": {"typeReachable": "org.jline.terminal.TerminalBuilder"}
      },
      {
        "pattern": "META-INF/native/windows64/jansi.dll$",
        "condition": {"typeReachable": "org.jline.terminal.TerminalBuilder"}
      }
    ]
  }
}

